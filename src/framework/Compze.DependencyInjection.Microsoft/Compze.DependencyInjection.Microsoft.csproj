<Project Sdk="Microsoft.NET.Sdk">

   <PropertyGroup>
      <TargetFramework>net8.0</TargetFramework>
      <Nullable>enable</Nullable>
   </PropertyGroup>

   <PropertyGroup>
      <RootNamespace>Compze</RootNamespace>
      <NuGetAuditMode>direct</NuGetAuditMode>
      <AnalysisLevel>latest-all</AnalysisLevel>
